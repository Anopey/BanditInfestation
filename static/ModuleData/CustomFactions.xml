  <Faction id="cs_redemptive_order"
          banner_key="1.142.91.3580.2745.776.703.1.0.0.500.116.116.310.314.773.752.1.1.0.416.116.116.119.120.773.751.0.1.-44.517.116.116.320.324.773.906.1.0.180.517.116.116.320.324.775.598.1.1.0.417.116.91.254.257.933.764.0.1.0.417.116.142.254.257.606.762.0.1.0.512.142.116.27.28.617.763.1.1.90.512.142.116.27.28.695.762.1.1.90.512.142.116.29.30.654.762.1.1.90.512.91.116.27.28.844.764.1.1.90.512.91.116.27.28.922.764.1.1.90.512.91.116.29.30.884.765.1.1.90"
           owner="Hero.cs_lord_redemptive_order_1"
           label_color="FF270078"
           color="FF4F2212"
